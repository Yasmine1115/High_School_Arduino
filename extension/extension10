#include <LiquidCrystal_I2C.h>
/*
  first name on first line
  last name in second line(use setcursor)
*/

LiquidCrystal_I2C lcd(0x27, 20, 4);

void setup() {
  lcd.init();
  lcd.backlight();
  lcd.setCursor(0,0);
  lcd.print("Yasmine");
  lcd.setCursor(0,1);
  lcd.print("Cabrera");
}

void loop() {
    
}
